<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.d1m.pandora.dao.MemberMapper">
	<select id="findByOpenid" parameterType="java.lang.String" resultType="cn.d1m.pandora.entry.Member">
		select open_id,nickname,language,city from member where open_id = #{open_id}
	</select>
	
</mapper>